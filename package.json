{
  "name": "eslint-plugin-raml",
  "version": "0.2.7",
  "description": "Eslint rules for RAML",
  "keywords": [
    "RAML",
    "eslint",
    "eslint-plugin",
    "eslintplugin"
  ],
  "bugs": {
    "url": "https://github.com/galk-in/eslint-plugin-raml/issues"
  },
  "license": "MIT",
  "author": "Nikita Galkin",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galk-in/eslint-plugin-raml.git"
  },
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "cs": "eslint lib/. test/. --ext .js",
