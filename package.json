    "test": "npm run cs && nyc npm run test-no-cs",
    "test-no-cs": "mocha"
  },
  "dependencies": {
    "raml-1-parser": "1.1.14",
    "raml-json-validation": "0.0.11"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.1.2",
    "nyc": "^10.1.2"
  },
  "engines": {
    "node": ">=4"
  },
  "url": "https://github.com/galk-in/eslint-plugin-raml"
